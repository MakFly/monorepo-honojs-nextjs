#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kev/Documents/boilerplate/monorepo/node_modules/.pnpm/@better-auth+cli@1.3.8-beta.10_@types+react@19.1.12_kysely@0.28.5_react-dom@19.1.1_react@19.1.1__react@19.1.1/node_modules/@better-auth/cli/dist/node_modules:/Users/kev/Documents/boilerplate/monorepo/node_modules/.pnpm/@better-auth+cli@1.3.8-beta.10_@types+react@19.1.12_kysely@0.28.5_react-dom@19.1.1_react@19.1.1__react@19.1.1/node_modules/@better-auth/cli/node_modules:/Users/kev/Documents/boilerplate/monorepo/node_modules/.pnpm/@better-auth+cli@1.3.8-beta.10_@types+react@19.1.12_kysely@0.28.5_react-dom@19.1.1_react@19.1.1__react@19.1.1/node_modules/@better-auth/node_modules:/Users/kev/Documents/boilerplate/monorepo/node_modules/.pnpm/@better-auth+cli@1.3.8-beta.10_@types+react@19.1.12_kysely@0.28.5_react-dom@19.1.1_react@19.1.1__react@19.1.1/node_modules:/Users/kev/Documents/boilerplate/monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kev/Documents/boilerplate/monorepo/node_modules/.pnpm/@better-auth+cli@1.3.8-beta.10_@types+react@19.1.12_kysely@0.28.5_react-dom@19.1.1_react@19.1.1__react@19.1.1/node_modules/@better-auth/cli/dist/node_modules:/Users/kev/Documents/boilerplate/monorepo/node_modules/.pnpm/@better-auth+cli@1.3.8-beta.10_@types+react@19.1.12_kysely@0.28.5_react-dom@19.1.1_react@19.1.1__react@19.1.1/node_modules/@better-auth/cli/node_modules:/Users/kev/Documents/boilerplate/monorepo/node_modules/.pnpm/@better-auth+cli@1.3.8-beta.10_@types+react@19.1.12_kysely@0.28.5_react-dom@19.1.1_react@19.1.1__react@19.1.1/node_modules/@better-auth/node_modules:/Users/kev/Documents/boilerplate/monorepo/node_modules/.pnpm/@better-auth+cli@1.3.8-beta.10_@types+react@19.1.12_kysely@0.28.5_react-dom@19.1.1_react@19.1.1__react@19.1.1/node_modules:/Users/kev/Documents/boilerplate/monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@better-auth/cli/dist/index.mjs" "$@"
else
  exec node  "$basedir/../@better-auth/cli/dist/index.mjs" "$@"
fi
